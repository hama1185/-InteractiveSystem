x = [3, 4, 2, 0, 1, 0, 5, 6, 7, 8, 9, 10 , 4.5, 3.5, 5.5, 6.8, 7.5, 9.5, 8.0, 2.5];//勉強時間
y = [80, 90, 78, 10, 45, 15, 80, 85, 88, 92, 94, 95, 83, 82, 80, 94, 90, 98, 92, 60];//試験の点

X = [];//既知パラメータ
ynear = [];//近似

for i = 1:20
    X = [X; log(x(i) + 1), 1];
end
a = pinv(X) * y';
for i = 0:0.1:10
    ynear = [ynear, a(1) * log(i + 1) + x(2)];
end

plot(x, y, 'o', [0:0.1:10], ynear);
legend("raw", "曲線近似");
xtitle("課題2","勉強時間","得点");